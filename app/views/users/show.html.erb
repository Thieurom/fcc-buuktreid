<% provide(:title, @user.username + " | Buuktreid") %>
<% content_for :sidebar do %>
  <%= render partial: 'menu', locals: { active_menu_link: 'Collection' } %>
<% end %>
<% content_for :detail do %>
  <% if @user.books.any? %>
    <ol class="list">
      <%= render @books %>
    </ol>
  <% end %>
<% end %>
